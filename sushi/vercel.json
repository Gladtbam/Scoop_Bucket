{
    "version": 2,
    "headers": [
        {
            "source": "/api",
            "headers": [
                {
                    "key": "cache-control",
                    "value": "s-maxage=28800"
                }
            ]
        }
    ],
    "build": {
        "env": {
            "DENO_UNSTABLE": "true"
        }
    },
    "functions": {
        "api/**/*.[jt]s": {
            "runtime": "vercel-deno@0.7.6"
        }
    },
    "github": {
        "enabled": false
    }
}
